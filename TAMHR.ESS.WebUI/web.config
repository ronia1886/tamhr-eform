<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.web>
    <customErrors mode="Off">
      <!-- Off, RemoteOnly, On -->
      <error statusCode="400" redirect="~/Views/Shared/Error" />
    </customErrors>
	<httpCookies requireSSL="true" httpOnlyCookies="true" ></httpCookies>
  </system.web>
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
    </handlers>
    <aspNetCore processPath="%LAUNCHER_PATH%" arguments="%LAUNCHER_ARGS%" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" requestTimeout="00:50:00" hostingModel="InProcess">
      <!--<aspNetCore processPath="%LAUNCHER_PATH%" arguments="%LAUNCHER_ARGS%" stdoutLogEnabled="true" stdoutLogFile=".\logs\stdout" forwardWindowsAuthToken="false">-->
		<environmentVariables>
			<environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Development" /> 
		</environmentVariables>
    </aspNetCore>
    <httpProtocol>
      <customHeaders>
        <remove name="X-Powered-By" />
        <add name="X-Frame-Options" value="SAMEORIGIN" />
        <!--<add name="X-Frame-Options" value="DENY" />-->
        <add name="X-XSS-Protection" value="1; mode=block" />
        <add name="X-Content-Type-Options" value="nosniff" />
        <add name="X-Permitted-Cross-Domain-Policies" value="none" />
        <add name="Referrer-Policy" value="same-origin" />
        <add name="Access-Control-Allow-Origin" value="*" />
        <add name="X-Powered-By" value="ASP.NET CORE 2.1" />
      </customHeaders>
    </httpProtocol>
    <modules>
      <remove name="ScriptModule-4.0" />
      <remove name="UrlAuthorization" />
      <remove name="OutputCache" />
      <remove name="FormsAuthentication" />
      <remove name="FileAuthorization" />
      <remove name="Session" />
      <remove name="RoleManager" />
      <remove name="Profile" />
      <remove name="DefaultAuthentication" />
      <remove name="WindowsAuthentication" />
    </modules>
  </system.webServer>
</configuration>
